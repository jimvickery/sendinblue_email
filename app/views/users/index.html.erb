<table border=1>
  <tr>
    <td> Name </td>
    <td> Email </td>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
  </tr>
  <% end %>
</table>
<%= link_to “Add User”,new_user_path %>
#users/new.html.erb
<%= form_for :user,url:users_path do |f| %>
  Name:<%= f.text_field :name%><br/>
  Email:<%= f.email_field :email%><br/>
  <%= f.submit %>
<% end %>